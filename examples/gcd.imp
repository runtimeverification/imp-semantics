fun mod(a, b) = {
  if (b == 0)
    return 0;
  else
    return - (a / b) * b + a;
}

fun gcd_(a, b) = {
  c = mod(a, b);
  if (c == 0)
    return b;
  else
    return gcd_(b, c);
}

fun gcd(a, b) = {
  if (a < 0)
    a = - a;
  if (b < 0)
    b = - b;
  if (b <= a)
    return gcd_(a, b);
  else
    return gcd_(b, a);
}

return gcd(51, -170);
